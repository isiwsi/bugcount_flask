$.ajax({
                    url:"/getUserList",
                    type:"post",
                    data:{"name":"李四","score":"99"},
                    datatype:"json",
                    success:function (d) {
                        $("h1").html("武汉感染人数" + d)
                    },error:function () {
                        alert("发送ajaxx失败")
                    }
                })

                 $("button[id='getUserList']").click(function (){
                console.log('前台点击了按钮');
                $.post('/getUserList');

              })

